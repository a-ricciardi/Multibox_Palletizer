<xs:simpleType name="WidgetId">
    <xs:annotation>
      <xs:documentation>
        Valid Widget ID. Has to start with a letter followed by letters, numbers or _ -
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <!-- we do not use \w for [A-Za-z0-9_], as the implementation of \w is not consistent over different tools -->
      <xs:pattern value="[A-Za-z]{1}[0-9A-Za-z_-]*"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="String">
    <xs:annotation>
      <xs:documentation>
        String Value
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Boolean">
    <xs:annotation>
      <xs:documentation>
        Boolean value. Possible options are true and false
      </xs:documentation>
    </xs:annotation>
    <!--<xs:restriction base="xs:boolean"></xs:restriction>-->
    <!-- xs:boolean erlaubt auch 0/1, wir nicht -->
    <xs:restriction base="xs:string">
      <xs:enumeration value="true"/>
      <xs:enumeration value="false"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Number">
    <xs:annotation>
      <xs:documentation>
        Any number, e.g. floating point value (e.g. 5.4) or integer (e.g. 5)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal" >
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Integer">
    <xs:annotation>
      <xs:documentation>
        Integer Value e.g. 5
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer" >
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Percentage">
    <xs:annotation>
      <xs:documentation>
        Floating point value in the range 0..100
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal" >
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="100" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DateTime">
    <xs:annotation>
      <xs:documentation>
        Date and Time string according to ECMA-262<br/>
        Examples:<br/>
        2017-06-21<br/>
        2017-06-21T17:59<br/>
        2017-06-21T17:59:00<br/>
        2017-06-21T17:59:00Z<br/>
        2017-06-21T17:59:00.000+03:00<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="(19|20)\d\d[-]((0[1-9])|(1[012]))[-]((0[1-9])|([12][0-9])|(3[01]))([T]((24:00(:00(\.000){0,1}){0,1})|(((0[0-9])|(1[0-9])|(2[0-3]))[:]([0-5][0-9])([:][0-5][0-9](\.\d\d\d){0,1}){0,1}))([Z]|([+-]((0[0-9])|(1[0-9])|(2[0-3])):[0-5][0-9])){0,1}){0,1}"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Time">
    <xs:annotation>
      <xs:documentation>
        time in ms
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer" >
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="brease.datatype.Notification">
    <xs:annotation>
      <xs:documentation>
        Notification for changes of Big Data. <br/>Notation as object with following poperties:<br/>
        <br/>
        •action: String (optional)<br/>
        <br/>
        •row: Number (optional)<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="brease.datatype.Node">
    <xs:annotation>
      <xs:documentation>
        Value with unit for binding with OPC-UA nodes. <br/>Notation as object with following properties:<br/>
        <br/>
        •id: String (optional)<br/>
        <br/>
        •maxValue: Number (optional)<br/>
        <br/>
        •minValue: Number (optional)<br/>
        <br/>
        •unit: String (required) <br/>
        common code of unit of values.<br/>
        <br/>
        •value: Number (required)<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="brease.datatype.ArrayNode">
    <xs:annotation>
      <xs:documentation>
        Array of values with one unit. <br/>Notation as object with following properties:<br/>
        <br/>
        •id: String (optional)<br/>
        <br/>
        •maxValue: Number (optional)<br/>
        <br/>
        •minValue: Number (optional)<br/>
        <br/>
        •unit: String (required) <br/>
        common code of unit of values.<br/>
        <br/>
        •value: Number[] (required)<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Object">
    <xs:annotation>
      <xs:documentation>
        Value in JSON Annotation e.g. {'key' : 'value'}
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="\{.*\}"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Array">
    <xs:annotation>
      <xs:documentation>
        Definition of Arrays in JSON Annotation, e.g. ['Item1', 'Item2'].
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="\[((('[\w ]*', *)|([0-9]*, *))*(|('[\w ]*')|([0-9]*))|)\]"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="IntegerList">
    <xs:annotation>
      <xs:documentation>
        List of Integer values separated by , e.g. 1,2,3,4,5
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="([0-9]{1,},{1} {0,1})*([0-9]{1,}){1}"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StringList">
    <xs:annotation>
      <xs:documentation>
        List of String values separated by , e.g. 'Item1', 'Item2', ...
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <!-- implementation of \w for [A-Za-z0-9_] is not consistent over different tools, therefore _ is included separate -->
      <xs:pattern value="('[ÀÈÌÒÙàèìòùÁÉÍÓÚÝáéíóúýÂÊÎÔÛâêîôûÃÑÕãñõÄËÏÖÜäëïöü¡¿çÇßØøÅåÆæÞþÐð\w\d\s_.,#@:?!()$/-]{1,}',{1} {0,1})*('[ÀÈÌÒÙàèìòùÁÉÍÓÚÝáéíóúýÂÊÎÔÛâêîôûÃÑÕãñõÄËÏÖÜäëïöü¡¿çÇßØøÅåÆæÞþÐð\w\d\s_.,#@:?!()$/-]{1,}'){1}"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Color">
    <xs:annotation>
      <xs:documentation>
        Single Color Value<br/>
        usage (Syntax like CSS):<br/>
        - HEX Color code: #FFFFFF<br/>
        - rgba Component syntax: rgba(100,100,100,1)
        - transparent
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="(#[0-9A-Fa-f]{6})|(#[0-9A-Fa-f]{4})|(#[0-9A-Fa-f]{3})|rgba\((((0)*((25[0-5])|(2[0-4][0-9])|(1[0-9][0-9])|([1-9][0-9])|([0-9]))|(0)+),(\s)?){3}((0(\.)?([0-9]){1,2})|0|1(\.0)?)\)|(transparent)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ColorList">
    <xs:annotation>
      <xs:documentation>
        list of colors, e.g. #FF8800,rgba(255,136,0,1)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="(((#[0-9A-Fa-f]{6}|#[0-9A-Fa-f]{3}|transparent|((rgba\((((25[0-5])|(2[01234][0-9])|([01][0-9][0-9])|([0-9]{1,2})),(\s)?){3}((0\.[0-9]{1,2})|1(\.0)?|0)\)))\s?){1,4}),( ){0,1})*((((#[0-9A-Fa-f]{6})|(#[0-9A-Fa-f]{3})|(rgba\((((25[0-5])|(2[01234][0-9])|([01][0-9][0-9])|([0-9]{1,2})),(\s)?){3}((0\.[0-9]{1,2})|1(\.0)?|0)\)))\s?){1,4}|(transparent))"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ColorCollection">
    <xs:annotation>
      <xs:documentation>
        Color Collection Value<br/>
        usage (Syntax like CSS):<br/>
        - One single value applies to all four sides e.g. #FFFFFF<br/>
        - Two values apply first to top and bottom, the second one to left and right e.g.  #FFFFFF #333333<br/>
        - Four values apply to top, right, bottom and left in that order (clockwise) e.g.  #FFFFFF #333333 #FFFFFF #333333<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="(((#[0-9A-Fa-f]{6})|(#[0-9A-Fa-f]{3})|(rgba\((((25[0-5])|(2[01234][0-9])|([01][0-9][0-9])|([0-9]{1,2})),(\s)?){3}((0\.[0-9]{1,2})|1(\.0)?|0)\)))\s?){1,4}|(transparent)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PixelVal">
    <xs:annotation>
      <xs:documentation>
        Single Pixel Value<br/>
        usage (Syntax like CSS) e.g. 2px<br/>
        allows additional % values: e.g. 50%<br/>
        allows additional zero without unit: 0<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="([0-9]{1,5}(%|px))|0"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StrictPixelVal">
    <xs:annotation>
      <xs:documentation>
        Pixel Value<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="\d+px"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StrictSignedPixelVal">
    <xs:annotation>
      <xs:documentation>
        Signed Pixel Value<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="[-+]?\d+px"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="HPos">
      <xs:restriction base="xs:string" >
        <xs:pattern value="left|center|right|\d+px"></xs:pattern>
      </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="VPos">
      <xs:restriction base="xs:string" >
        <xs:pattern value="top|middle|bottom|\d+px"></xs:pattern>
      </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PixelValCollection">
    <xs:annotation>
      <xs:documentation>
        Pixel Collection Value<br/>
        usage (Syntax like CSS):<br/>
        - One single value applies to all four sides e.g. 2px<br/>
        - Two values apply first to top and bottom, the second one to left and right e.g.  1px 2px<br/>
        - Four values apply to top, right, bottom and left in that order (clockwise) e.g.  1px 2px 3px 4px<br/>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" >
      <xs:pattern value="(([0-9]{1,5}(%|px))|0)( (([0-9]{1,5}(%|px))|0)){0,3}"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Size">

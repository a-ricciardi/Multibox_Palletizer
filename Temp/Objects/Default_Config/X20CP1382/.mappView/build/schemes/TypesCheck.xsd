      <xs:documentation>
        Size Value<br/>
        usage:<br/>
        - definition as an Integer e.g. 40<br/>
        - definition percentage value e.g. 40%
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="types:AutoSize" >
      <xs:pattern value="(([^\-]{0,1})?([0-9]{1,}))|([0-9]{1,4}%)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

